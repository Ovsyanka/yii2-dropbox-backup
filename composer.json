{
    "name": "demi/dropbox-backup",
    "description": "Yii2 console command for making site backups and upload it to your dropbox account",
    "keywords": [
        "yii2",
        "dropbox",
        "backup",
        "upload",
        "sync",
        "dump"
    ],
    "homepage": "https://github.com/demisang/yii2-dropbox-backup#readme",
    "type": "yii2-extension",
    "license": "GNU",
    "authors": [
        {
            "name": "Ivan Orlov",
            "email": "orlov_mail@mail.ru",
            "homepage": "https://orlov.io/",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/demisang/yii2-dropbox-backup/issues",
        "source": "https://github.com/demisang/yii2-dropbox-backup"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/demisang/dropbox-sdk-php"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "demi/backup": "*",
        "dropbox/dropbox-sdk": "dev-v1.1.8"
    },
    "suggest": {
        "php-64bit": "The Dropbox SDK uses 64-bit integers, this could cause problems because some of the numbers (file sizes, quota, etc) can be larger than 32-bit ints can handle. http://stackoverflow.com/a/864402"
    },
    "autoload": {
        "psr-4": {
            "demi\\backup\\dropbox\\": ""
        }
    }
}
